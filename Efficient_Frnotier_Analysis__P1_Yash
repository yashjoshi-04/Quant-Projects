   "source": [
    "\n",
    "def portfolio_variance(Rp, alpha, zeta, delta):\n",
    "    R_mv = alpha / delta  # Minimum variance portfolio return\n",
    "    term1 = 1 / delta\n",
    "    term2 = delta * (Rp - R_mv) ** 2 / (zeta * delta - alpha ** 2)\n",
    "    return term1 + term2\n",
    "\n",
    "\n",
    "def variance_of_returns_with_riskless_asset(Rp, alpha, zeta, delta, Rf):\n",
    "    Term_A = (Rp - Rf)**2\n",
    "    Term_B = zeta - (2 * alpha * Rf) + (delta * (Rf**2))\n",
    "    Variance_with_riskless_asset = Term_A / Term_B\n",
    "    return Variance_with_riskless_asset\n",
    "\n",
    "\n",
    "def calculate_frontier_params(mean_returns, cov_matrix):\n",
    "    ones_vector = np.ones(len(mean_returns))\n",
    "    inv_cov_matrix = np.linalg.inv(cov_matrix)\n",
    "    \n",
    "    alpha = ones_vector.T @ inv_cov_matrix @ mean_returns\n",
    "    zeta = mean_returns.T @ inv_cov_matrix @ mean_returns\n",
    "    delta = ones_vector.T @ inv_cov_matrix @ ones_vector\n",
    "    return alpha, zeta, delta\n",
    "\n",
    "def plot_min_variance_frontier(mean_returns, cov_matrix):\n",
    "    alpha, zeta, delta = calculate_frontier_params(mean_returns, cov_matrix)\n",
    "\n",
    "\n",
    "    R_mv = alpha / delta\n",
    "\n",
    "    Rp_values = np.linspace(0, 0.05, 100)\n",
    "\n",
    "    std_devs = [np.sqrt(portfolio_variance(Rp, alpha, zeta, delta)) for Rp in Rp_values]\n",
    "\n",
    "    plt.figure(figsize=(18, 8))\n",
    "    plt.plot(std_devs, Rp_values, label=\"Efficient Frontier without Riskless Asset\", color=\"black\")\n",
    "    plt.axhline(y=R_mv, color='black', linestyle='--', label=\"Rmv\")\n",
    "\n",
    "\n",
    "    Rf = 0.00013\n",
    "    plt.axhline(y=Rf, color='red', linestyle='--', linewidth=1.5, label=\"Risk-free Rate (Rf = 0.13%)\")\n",
    "\n",
    "\n",
    "    std_devs_with_riskless = np.sqrt(variance_of_returns_with_riskless_asset(Rp_values, alpha, zeta, delta, Rf))\n",
    "\n",
    "\n",
    "    intersection_index = np.argmin(np.abs(np.array(std_devs) - np.array(std_devs_with_riskless)))\n",
    "    intersection_std = std_devs[intersection_index]\n",
    "    intersection_Rp = Rp_values[intersection_index]\n",
    "\n",
    "    plt.text(intersection_std, intersection_Rp, 'T', fontsize=15, color=\"blue\", ha='center', va='center')\n",
    "    \n",
    "    plt.plot(std_devs_with_riskless, Rp_values, label=\"Efficient Frontier with Riskless Asset\", color=\"blue\", linestyle=\"-.\")\n",
    "\n",
    "    x_ticks = np.concatenate((np.linspace(0, 0.020, 5), np.arange(0.025, 0.051, 0.005)))\n",
    "    plt.xticks(x_ticks)\n",
    "\n",
    "    x_labels = [f'{x:.3f}' for x in x_ticks]\n",
    "    plt.gca().set_xticklabels(x_labels)\n",
    "\n",
    "    plt.xlim([0, 0.050])\n",
    "    plt.ylim([-0.005, 0.02])\n",
    "\n",
    "    plt.xlabel('Standard Deviation (Ïƒ)', fontsize=14)\n",
    "    plt.ylabel('Expected Monthly Return', fontsize=14)\n",
    "    plt.title('Efficient Frontier with and without Riskless Asset', fontsize=18)\n",
    "    plt.legend(fontsize=12, loc=\"upper left\")\n",
    "    plt.minorticks_on()\n",
    "\n",
    "    plt.xticks(np.arange(0.020, 0.051, 0.005))\n",
    "    plt.grid(True, which='both', linestyle='-', linewidth=0.5)\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "plot_min_variance_frontier(mean_returns, cov_matrix)\n"
   ]
  },
