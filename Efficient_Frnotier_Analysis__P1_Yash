  {
   "cell_type": "code",
   "execution_count": 86,
   "id": "c5d9de0f-231b-4e6c-9573-0773b7e1c449",
   "metadata": {},
   "outputs": [],
   "source": [
    "#defining the formula and adding alpha, zeta and delta\n",
    "def portfolio_variance(Rp, alpha, zeta, delta):\n",
    "    R_mv = alpha / delta  # Minimum variance portfolio return\n",
    "    term1 = 1 / delta\n",
    "    term2 = delta * (Rp - R_mv) ** 2 / (zeta * delta - alpha ** 2)\n",
    "    return term1 + term2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 87,
   "id": "2f21c1bb-e6c6-4c1f-8f6d-8a59bc893603",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1500x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "#Plotting the minimum-variance frontier\n",
    "def plot_min_variance_frontier(mean_returns, cov_matrix):\n",
    "    alpha, zeta, delta = calculate_frontier_params(mean_returns, cov_matrix)\n",
    "\n",
    "    #Calculating the return of the minimum variance portfolio\n",
    "    R_mv = alpha / delta\n",
    "\n",
    "    #Generating a range of expected returns (Rp) from the minimum return (0) up to a chosen maximum (0.02)\n",
    "    Rp_values = np.linspace(0, 0.05, 1000)\n",
    "\n",
    "    #Calculating the corresponding standard deviations\n",
    "    std_devs = [np.sqrt(portfolio_variance(Rp, alpha, zeta, delta)) for Rp in Rp_values]\n",
    "\n",
    "    #Plotting the minimum-variance frontier\n",
    "    plt.figure(figsize=(15, 8))\n",
    "    plt.plot(std_devs, Rp_values, label=\"Efficient Frontier\", color=\"black\")\n",
    "\n",
    " \n",
    "    plt.axhline(y=R_mv, color='black', linestyle='--', label=\"Rmv\")\n",
    "\n",
    "    plt.xlabel('Standard Deviation (Ïƒ)')\n",
    "    plt.ylabel('Expected Monthly Return')\n",
    "    plt.title('Efficient Frontier')\n",
    "\n",
    "\n",
    "    plt.xticks(np.arange(0.020, 0.051, 0.005))\n",
    "    plt.xlim([0.020, 0.050])\n",
    "    plt.ylim([0, 0.02])\n",
    "    plt.grid(True)\n",
    "    plt.legend()\n",
    "    plt.show()\n",
    "\n",
    "# Using the mean returns and covariance matrix to plot the frontier\n",
    "mean_returns_vector = mean_returns.values\n",
    "cov_matrix_values = cov_matrix.values\n",
    "\n",
    "# Plot the minimum variance frontier\n",
    "plot_min_variance_frontier(mean_returns_vector, cov_matrix_values)"
   ]
  },
