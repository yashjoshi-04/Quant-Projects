   "source": [
    "mean_returns = returns_data.mean()\n",
    "std_devs = returns_data.std()\n",
    "\n",
    "\n",
    "cov_matrix = returns_data.cov()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "id": "a68388ed-0756-418a-88dc-1839241cd688",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Calculating alpha, delta and zeta\n",
    "def calculate_frontier_params(mean_returns, cov_matrix):\n",
    "    inv_cov_matrix = np.linalg.inv(cov_matrix)\n",
    "    ones = np.ones(len(mean_returns))\n",
    "\n",
    "    alpha = mean_returns.T @ inv_cov_matrix @ ones\n",
    "    zeta = mean_returns.T @ inv_cov_matrix @ mean_returns\n",
    "    delta = ones.T @ inv_cov_matrix @ ones\n",
    "\n",
    "    return alpha, zeta, delta"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 84,
   "id": "6a77e310-0971-4358-ab1f-62b696297234",
   "metadata": {},
   "outputs": [],
   "source": [
    "inv_cov_matrix = np.linalg.inv(cov_matrix)\n",
    "ones = np.ones(len(mean_returns))\n",
    "\n",
    "alpha = mean_returns.T @ inv_cov_matrix @ ones\n",
    "zeta = mean_returns.T @ inv_cov_matrix @ mean_returns\n",
    "delta = ones.T @ inv_cov_matrix @ ones"
   ]
  },
