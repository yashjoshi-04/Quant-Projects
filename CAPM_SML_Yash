    "Durables = pd.DataFrame(industry['Durbl']) - Rf\n",
    "Manufacturing = pd.DataFrame(industry['Manuf']) - Rf\n",
    "Energy = pd.DataFrame(industry['Enrgy']) - Rf\n",
    "HiTec = pd.DataFrame(industry['HiTec']) - Rf\n",
